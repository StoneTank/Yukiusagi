      <setting name="StatusesCount" serializeAs="String">
        <value>200</value>
      </setting>
      <setting name="Font" serializeAs="String">
        <value>Meiryo UI, 9pt</value>
      </setting>
      <setting name="PostShortcutKey" serializeAs="String">
        <value>Shift+Enter</value>
      </setting>
      <setting name="WindowState" serializeAs="String">
        <value>Normal</value>
      </setting>
      <setting name="SplitterDistance" serializeAs="String">
        <value>72</value>
      </setting>
      <setting name="NotificationEnabled" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="WindowLocation" serializeAs="String">
        <value>100, 50</value>
      </setting>
      <setting name="IsUpgraded" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="WindowSize" serializeAs="String">
        <value>800, 600</value>
      </setting>
      <setting name="WindowLayout" serializeAs="String">
        <value />
      </setting>
      <setting name="TabId" serializeAs="String">
        <value>0</value>
      </setting>
    </StoneTank.Yukiusagi.Properties.Settings>
